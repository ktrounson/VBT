Ugle_HaganB2$date = as.Date(Ugle_HaganB2$date, format = "%d/%m/%y")
Ugle_HaganB3$date = as.Date(Ugle_HaganB3$date, format = "%d/%m/%y")

ggplot(NULL, aes(date, Conc.Mean.Velocity)) +
  geom_point(data = Ugle_HaganB2,
             aes(shape = exercise, col = "blue"), size = 3) +
  geom_point(data = Ugle_HaganB3,
             aes(shape = exercise, col = "red"), size = 3) +
  ylim(0.4, 1) +
  labs(shape="Exercise",colour="Weight") +
  scale_color_manual(name="Weight", labels=c("95-105 kg","125-135 kg"),
                     values=c("blue","red")) +
  xlab("Date") +
  ylab("Concentric Mean Velocity") +
  guides(colour = guide_legend(order = 1),
         shape = guide_legend(order = 2)) +
  geom_hline(yintercept=0.7528571) +
  geom_hline(yintercept=0.7528571+0.08, color = "blue") +
  geom_hline(yintercept=0.7528571-0.08, color = "blue") +
  geom_hline(yintercept=0.9387431, color = "red") +
  geom_hline(yintercept=0.5669712, color = "red") +
  ggtitle("Ugle-Hagan set mean")

