#Bedendo
#Mar 87.5-97.5, Jun 97.5-107.5
BedendoB2 <- BedendoB2[BedendoB2$Conc.Mean.Velocity != 0.22, ]
BedendoB3 <- BedendoB3[BedendoB3$Conc.Mean.Velocity != 0.39, ]
Bedendomean <- rbind.data.frame(BedendoB2, BedendoB3)
mean(Bedendomean$Conc.Mean.Velocity)
mean(Bedendomean$Conc.Mean.Velocity) + 1.5*sd(Bedendomean$Conc.Mean.Velocity)
mean(Bedendomean$Conc.Mean.Velocity) - 1.5*sd(Bedendomean$Conc.Mean.Velocity)

BedendoB2$date = as.Date(BedendoB2$date, format = "%d/%m/%y")
BedendoB3$date = as.Date(BedendoB3$date, format = "%d/%m/%y")

ggplot(NULL, aes(date, Conc.Mean.Velocity)) +
  geom_point(data = BedendoB2,
             aes(shape = exercise, col = "blue"), size = 3) +
  geom_point(data = BedendoB3,
             aes(shape = exercise, col = "red"), size = 3) +
  ylim(0.4, 1) +
  labs(shape="Exercise",colour="Weight") +
  scale_color_manual(name="Weight", labels=c("87.5-97.5 kg","97.5-107.5 kg"),
                     values=c("blue","red")) +
  xlab("Date") +
  ylab("Concentric Mean Velocity") +
  guides(colour = guide_legend(order = 1),
         shape = guide_legend(order = 2)) +
  geom_hline(yintercept=0.7857143) +
  geom_hline(yintercept=0.7857143+0.08, color = "blue") +
  geom_hline(yintercept=0.7857143-0.08, color = "blue") +
  geom_hline(yintercept=0.9662629, color = "red") +
  geom_hline(yintercept=0.6051657, color = "red") +
  ggtitle("Bedendo set mean")

#Bontempelli
#Jan 117.5-127.5, Mar 135-145, Jun 155-165
BontempelliB1 <- BontempelliB1[BontempelliB1$Conc.Mean.Velocity != 1.08, ]
Bontempellimean <- rbind.data.frame(BontempelliB1, BontempelliB2, BontempelliB3)
mean(Bontempellimean$Conc.Mean.Velocity)
mean(Bontempellimean$Conc.Mean.Velocity) + 1.5*sd(Bontempellimean$Conc.Mean.Velocity)
mean(Bontempellimean$Conc.Mean.Velocity) - 1.5*sd(Bontempellimean$Conc.Mean.Velocity)

BontempelliB1$date = as.Date(BontempelliB1$date, format = "%d/%m/%y")
BontempelliB2$date = as.Date(BontempelliB2$date, format = "%d/%m/%y")
BontempelliB3$date = as.Date(BontempelliB3$date, format = "%d/%m/%y")

ggplot(NULL, aes(date, Conc.Mean.Velocity)) +
  geom_point(data = BontempelliB1,
             aes(shape = exercise, col = "blue"), size = 3) +
  geom_point(data = BontempelliB2,
             aes(shape = exercise, col = "green"), size = 3) +
  geom_point(data = BontempelliB3,
             aes(shape = exercise, col = "red"), size = 3) +
  ylim(0.4, 1) +
  labs(shape="Exercise",colour="Weight") +
  scale_color_manual(name="Weight", labels=c("117.5-127.5 kg","135-145 kg", "155-165 kg"),
                     values=c("green","blue","red")) +
  xlab("Date") +
  ylab("Concentric Mean Velocity") +
  guides(colour = guide_legend(order = 1),
         shape = guide_legend(order = 2)) +
  geom_hline(yintercept=0.7217391) +
  geom_hline(yintercept=0.7217391+0.08, color = "blue") +
  geom_hline(yintercept=0.7217391-0.08, color = "blue") +
  geom_hline(yintercept=0.8632237, color = "red") +
  geom_hline(yintercept=0.5802546, color = "red") +
  ggtitle("Bontempelli set mean")

#Butler
#Mar 92.5-102.5, Jun 92.5-102.5
Butlermean <- rbind.data.frame(ButlerB2, ButlerB3)
mean(Butlermean$Conc.Mean.Velocity)
mean(Butlermean$Conc.Mean.Velocity) + 1.5*sd(Butlermean$Conc.Mean.Velocity)
mean(Butlermean$Conc.Mean.Velocity) - 1.5*sd(Butlermean$Conc.Mean.Velocity)

ButlerB2$date = as.Date(ButlerB2$date, format = "%d/%m/%y")

ggplot(NULL, aes(date, Conc.Mean.Velocity)) +
  geom_point(data = ButlerB2,
             aes(shape = exercise, col = "blue"), size = 3) +
  ylim(0.4, 1) +
  labs(shape="Exercise",colour="Weight") +
  scale_color_manual(name="Weight", labels=c("92.5-102.5 kg"),
                     values=c("blue")) +
  xlab("Date") +
  ylab("Concentric Mean Velocity") +
  guides(colour = guide_legend(order = 1),
         shape = guide_legend(order = 2)) +
  geom_hline(yintercept=0.68375) +
  geom_hline(yintercept=0.68375+0.08, color = "blue") +
  geom_hline(yintercept=0.68375-0.08, color = "blue") +
